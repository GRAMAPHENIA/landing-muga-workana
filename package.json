  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@testing-library/dom": "^10.4.1",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.18.0",
    "eslint-plugin-astro": "^1.3.1",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
